<Window x:Class="FunctionsViewUITester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FunctionsViewUITester"
        xmlns:FunctionsView="clr-namespace:FunctionsView.View;assembly=FunctionsView"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <!--<curveGen:CurveGeneratorTable Grid.Row="0" />-->
        <FunctionsView:CoordinatesFunctionEditor Grid.Row="1" Function="{Binding Path=Function}"/>
        <Button Grid.Row="2"  Content="OK" Width="60" Height="20" Click="Button_Click" />
    </Grid>
</Window>
