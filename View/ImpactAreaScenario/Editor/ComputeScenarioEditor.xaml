<UserControl x:Class="HEC.FDA.View.ImpactAreaScenario.Editor.ComputeScenarioEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mvvmView="clr-namespace:HEC.MVVMFramework.View.UserControls;assembly=View"
             xmlns:local="clr-namespace:HEC.FDA.View.ImpactAreaScenario.Editor"            
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Name="myGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <mvvmView:SelectableMessageView  Grid.Row="0"/>
        <Button Content="Close" Grid.Row="1" Width="40" HorizontalAlignment="Right" Margin="5" />
    </Grid>
    
</UserControl>