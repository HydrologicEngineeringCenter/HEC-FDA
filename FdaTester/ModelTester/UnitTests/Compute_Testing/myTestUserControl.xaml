        <TextBox Name="txt_box" Text="This is a test" Grid.Row="0" Width="100" Margin="10" />
        <Button Name="btn_myButton" Content="click me" Width="60" Grid.Row="1" Margin="10" />
    </Grid>
</UserControl>
